#include <stdio.h>
#include<stdlib.h>
#define INF 999
#define v 6
void print(int a[][v]);
void floydWarshall(int graph[][v]) 
{
    	int a[v][v], i, j, k;
    	for (i=0; i<v; i++)
    	{
        		for (j=0; j<v; j++)
        		{
           		 a[i][j] = graph[i][j];
        		}
    	}
    	for(k=0; k<v; k++)          
    	{
        		for(i=0; i<v; i++) 
        		{
            		for(j=0; j<v; j++) 
            		{
                			if((a[i][k]+a[k][j])< a[i][j])
                			{
                   				a[i][j]= a[i][k]+a[k][j];
                			}
            		}
        		}
        		print(a);
    	}
}

void print(int a[][v]) 
{
    	printf("\n");
    	for (int i = 0; i < v; i++) 
    	{
        		for (int j = 0; j < v; j++) 
        		{
            		if (a[i][j] == 999)
                			printf("%4s", "INF");
            		else
               			printf("%4d", a[i][j]);
        		}
        		printf("\n");
    	}
}


int main() 
{
    	int graph[v][v];
    	printf("No of vertices: %d ",v);
    	printf("\nEnter the weight matrix\n State infinity as 999\n");
    	for(int i =0;i<v;i++)
    	{
        		for(int j=0;j<v;j++)
        		{
            		scanf("%d", &graph[i][j]);
        		}
    	}
    	floydWarshall(graph);
}
